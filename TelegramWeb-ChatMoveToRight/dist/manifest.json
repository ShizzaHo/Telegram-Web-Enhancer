{
    "manifest_version": 2,
    "name": "TelegramWeb-ChatMoveToRight",
    "description": "Смещение чата в Telegram WEB Z вправо",
    "version": "1.0.0",
    "permissions": ["storage", "activeTab"],
    "content_scripts": [
        {
          "matches": [
            "https://web.telegram.org/*"
          ],
          "js": ["customizator.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
  }